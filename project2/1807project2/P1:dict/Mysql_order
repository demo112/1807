create database dict default charset=utf8;
use dict;

	create table user (
	id int(6) primary key auto_increment, 
	name varchar(32) not null, 
	passwd varchar(16) default '000000');
	desc user;

	create table hist(
	id int(6) primary key auto_increment, 
	name varchar(32) not null, 
	word varchar(32) not null, 
	time varchar(64));
	desc hist;

	create table words(
	id int(6) primary key auto_increment, 
	word varchar(32) not null, 
	interpret text not null);
	desc words;
show tables;

